#Index View
<h1> This is the Index View </h1>

<% @posts.each do |post| %>
    <p>
        <label>ID: </label>
        <%= post.id %>
    </p>
    <p>
        <label>Title: </label>
        <%= post.title %>
    </p>
    <p>
        <label>Post Text: </label>
        <%= post.body %>
    </p>
<% end %>